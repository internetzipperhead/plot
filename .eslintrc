{
  "extends": "react-app",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-undef":"off",
    "semi": [2, "never"],
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "anchor-is-valid": 0,
    "jsx-a11y/anchor-is-valid": 0
  }
}
